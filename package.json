  "bugs": {
    "url": "https://github.com/Build-Week-Potluck-Planner-2-0/back-end/issues"
  },
  "homepage": "https://github.com/Build-Week-Potluck-Planner-2-0/back-end#readme",
  "engines": {
    "node": "16.13.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
